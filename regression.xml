<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression" parallel="tests" thread-count="2">
    <test verbose="2" name="Regression on chrome" parallel="classes" thread-count="2">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="login.LoginTests"/>
            <class name="login.LoginTests1"/>
        </classes>
    </test>
    <test verbose="2" name="Regression on firefox" parallel="classes" thread-count="2">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="login.LoginTests">
                <methods>
                    <exclude name="testSuccessfulLogin2"/>
                </methods>
            </class>
            <class name="login.LoginTests1"/>
        </classes>
    </test>
</suite>